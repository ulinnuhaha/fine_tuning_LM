    "# metrics\n",
    "import evaluate\n",
    "rouge_score = evaluate.load(\"rouge\")\n",
    "bleu_score = evaluate.load(\"bleu\")\n",
    "chrf_score = evaluate.load(\"chrf\")\n",
    "sacrebleu_score = evaluate.load(\"sacrebleu\")"
   ]
